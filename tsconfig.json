{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*":["src/components/*"],
      "@sections/*":["src/sections/*"],
      "@layouts/*":["src/layouts/*"],
      "@pages/*":["src/pages/*"],
      "@styles/*":["src/styles/*"],
      "@utils/*":["src/utils/*"],
      "@lib/*":["src/lib/*"]
    }
  }
}
